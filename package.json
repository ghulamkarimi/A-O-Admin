{
"name": "admin-panel", 
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS='--no-deprecation' vite dev -p 4173",
    "build": "tsc -b && vite build",  
    "lint": "eslint .",  
    "start": "vite preview",  
    "setup": "npm i && npm run build && pm2 start --name admin-panel npm -- start --update-env --time && pm2 save",
    "deploy": "git pull --no-rebase && npm i && npm run build && pm2 restart admin-panel --update-env --time && pm2 save", 
    "cp": "node cli/cp.mjs"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.4.0",
    "axios": "^1.7.8",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^7.0.1",
    "react-toastify": "^10.0.6",
    "socket.io-client": "^4.8.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-big-calendar": "^1.16.0",
    "@types/react-calendar": "^4.1.0",
    "@types/react-dom": "^18.3.1",
    "@types/react-toastify": "^4.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "globals": "^15.12.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
